{% extends 'admin/base.html.twig' %}
{% block title %}Modification de la Team &nbsp {{team.nom}}{% endblock %}

{% form_theme myForm 'bootstrap_4_layout.html.twig' %}
{% block body %}
    <div class="container-fluid">
        <h1 class="my-5">Modifier la Team &nbsp {{team.nom}}</h1>
        {{form_start(myForm)}}
        {{form_widget (myForm)}}
       
           <button type="submit" class="btn btn-success">
                    <i class="fas fa-check"></i> Enregistrer les modifications
                </button>
        {{form_end(myForm)}}
      
    </div>
{% endblock %}